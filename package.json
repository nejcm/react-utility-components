  "description": "React utility components.",
  "source": "src/index.ts",
  "main": "dist/@nejcm/react-utility-components.cjs.js",
  "umd:main": "dist/@nejcm/react-utility-components.umd.js",
  "module": "dist/@nejcm/react-utility-components.esm.js",
  "types": "types/index.d.ts",
  "sideEffects": false,
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/nejcm/react-utility-components"
  },
  "bugs": {
    "url": "https://github.com/nejc/react-utility-components/issues"
  },
  "homepage": "https://github.com/nejcm/react-utility-components",
  "engines": {
    "node": ">=8",
    "npm": ">=5"
  },
  "keywords": [
    "js",
    "react",
    "utility",
    "components",
    "shared",
    "library"
  ],
  "files": [
    "dist",
    "types"
  ],
  "scripts": {
    "storybook": "start-storybook -p 9001 -c .storybook",
    "storybook:build": "build-storybook -c .storybook -o .storybook-static",
    "storybook:deploy": "npm run storybook:build && gh-pages -d .storybook-static",
    "clean": "rimraf dist coverage .storybook-static",
    "format": "prettier ./src --write",
    "lint": "eslint ./src/**/*.{js,jsx,ts,tsx}",
    "lint:fix": "npm run lint --fix",
    "test": "npm run lint && jest",
    "test:cov": "jest --coverage --colors && jest-coverage-badges",
    "test:build": "jest --coverage --colors --silent && jest-coverage-badges",
    "test:watch": "jest --watch",
    "tsc": "tsc",
    "tsc:watch": "tsc --watch",
    "tsc:declaration": "tsc -p tsconfig.json --emitDeclarationOnly",
    "build": "npm run clean && kcd-scripts build --bundle --no-clean --size-snapshot && npm run tsc:declaration",
    "validate": "npm run format && npm run lint && npm run test:build",
    "setup": "npm install && npm run validate",
    "semantic-release": "semantic-release"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "peerDependencies": {
    "react": ">=16.8",
    "styled-components": ">=4"
  },
  "dependencies": {
    "@nejcm/js-helpers": "^4.0.0"
